<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.gestionClasse.model.Prof" table="Prof">
		<id name="codeprof" column="codeProf" length="10"></id>
		<property name="nom" column="nomProf" length="50"></property>
		<property name="prenom" column="prenomProf" length="25"></property>
		<property name="grade" column="gradeProf" length="25"></property>
	</class>
</hibernate-mapping>